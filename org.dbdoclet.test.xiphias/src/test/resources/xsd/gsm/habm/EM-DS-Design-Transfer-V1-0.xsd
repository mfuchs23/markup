			<xs:element name="DesignCurrentStatusDate" type="xs:date" minOccurs="0"/>
			<xs:element name="OtherDateTime" type="OtherDateTimeType" minOccurs="0"/>
			<xs:element name="DesignRepresentationSheetDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DesignRepresentationSheet" type="DesignRepresentationSheetType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="IndicationProductDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="IndicationProduct" type="TextType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ClassDescriptionDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ClassDescription" type="ClassDescriptionType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="PriorityDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Priority" type="PriorityType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExhibitionPriorityDetails" minOccurs="0">
