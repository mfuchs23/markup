	<!-- ********************************************************************** -->
	<!-- ********************************************************************** -->
	<!-- * TRANSACTION/MESSAGE TYPES LIBRARY ********************************** -->
	<!-- ********************************************************************** -->
	<!-- ********************************************************************** -->
	<!-- ====================================================================== -->
	<!-- Root Messages                                                          -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Top Level Transaction for all Types of Message                         -->
	<!-- ====================================================================== -->
     <xs:element name="Design" type="DesignType"/>						<!--DPMAHABMFS-->                           
	<!-- ====================================================================== -->
		<!--      : HABMFSType               added DPMAHABMFS                       -->
		<!-- ====================================================================== -->
		<xs:complexType name="HABMFSType">
				  <xs:sequence> 
				    <xs:element name="Bestandsart" type="BestandsartEnum" minOccurs="0" />				   		
				    <xs:element name="ApplicationNumberSortierschluessel"   type="xs:string" minOccurs="0"/>
					<xs:element name="DesignIdentifierSortierschluessel"   type="xs:string" minOccurs="0"/>					
				    <xs:element name="Datenbestand" type="xs:string" minOccurs="0" /> 
				    <xs:element name="Bild" type="xs:string" minOccurs="0" /> 
				    <xs:element name="Thumb" type="xs:string" minOccurs="0" /> 
				    				    
				    <xs:element name="FirstImportDate" type="xs:date" minOccurs="1" /> 
				    <xs:element name="History" type="HistoryType" minOccurs="1" /> 	
		    </xs:sequence>
		</xs:complexType>
		<!-- ====================================================================== -->
		<!--      : HistoryType               added DPMAHABMFS                      -->
		<!-- ====================================================================== -->
		
			<xs:complexType name="HistoryType">
				<xs:sequence>
					<xs:element name="HistoryRecord" minOccurs="1"  maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
									<xs:element name="ImportFilenameDate"  type="xs:date"  minOccurs="1"  maxOccurs="1" > <!-- unused/info only -->
									</xs:element>
		   							<xs:element name="ImportDate"  type="xs:date" minOccurs="1"  maxOccurs="1" >           <!-- used for Kurier --> 		   		
		   							</xs:element>						
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:complexType>
			
		<!-- ====================================================================== -->
		<!--    BestandsartEnum :                 added DPMAHABMFS                  -->
		<!-- ====================================================================== -->
		<xs:simpleType name="BestandsartEnum">
		        <xs:restriction base="xs:token">
		            <xs:enumeration value="aktiv"/>
		            <xs:enumeration value="nichtaktiv"/>
		        </xs:restriction>
		</xs:simpleType>    
	
	<!-- ====================================================================== -->
	<!-- Filename    : TransactionBodyType.xsd                                  -->
	<!-- Author      : EM-DS-Design                                             -->
	<!-- Version     : 1.0                                                      -->
	<!-- Last Update : 2011-16-05                                               -->
	<!-- ====================================================================== -->
	<xs:complexType name="TransactionBodyType">
		<xs:sequence>
			<xs:element name="TransactionContentDetails">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TransactionIdentifier" type="xs:token"/>
						<xs:element name="TransactionCode" type="TransactionCodeType"/>
						<xs:element name="TransactionData" type="TransactionDataType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ====================================================================== -->
	<!-- Filename    : TransactionDataType.xsd                                  -->
	<!-- Author      : EM-DS-Design                                             -->
	<!-- Version     : 1.0                                                      -->
	<!-- Last Update : 2011-16-05                                               -->
	<!-- ====================================================================== -->
	<xs:complexType name="TransactionDataType">
		<xs:sequence>
			<!-- ================================================================= -->
			<!-- Set of Designs                                                    -->
			<!-- ================================================================= -->
			<xs:element name="DesignDetails">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Design" type="DesignType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- ================================================================= -->
			<!-- Set of Appeals                                                    -->
			<!-- ================================================================= -->
			<xs:element name="AppealDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Appeal" type="AppealType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<!-- ================================================================= -->
			<!-- Set of Decisions                                                  -->
			<!-- ================================================================= -->
			<xs:element name="DecisionDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Decision" type="DecisionType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ====================================================================== -->
	<!-- Type Name : AppealType.xsd                                             -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- EM-DS-Design                                                           -->
	<!-- ====================================================================== -->
	<xs:complexType name="AppealType">
		<xs:sequence>
			<xs:element name="AppealIdentifier" type="xs:string" minOccurs="0"/>
			<xs:element name="AppealURI" type="URIType" minOccurs="0"/>
			<xs:element name="AppealDate" type="xs:date" minOccurs="0"/>
			<xs:element name="AppealDecisionDate" type="xs:date" minOccurs="0"/>
			<xs:element name="AppealProceedingLanguageCode" type="EM_EUOfficialLanguageCodeType" minOccurs="0"/>
			<xs:element name="CaseReferenceDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CaseReference" type="CaseReferenceType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AppealNickname" type="xs:string" minOccurs="0"/>
			<xs:element name="AppealDecisionStatusCode" type="xs:string" minOccurs="0"/>
			<xs:element name="AppellantDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AppellantKey" type="KeyType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RepresentativeDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RepresentativeKey" type="KeyType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RespondentDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RespondentKey" type="KeyType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="RespondentRepresentativeDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="RespondentRepresentativeKey" type="KeyType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AppealEventDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AppealEvent" type="AppealEventType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AppealDocumentDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AppealDocument" type="DocumentType" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ====================================================================== -->
	<!-- Type Name : AppealEventType                                            -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- EM-DS-Design                                                           -->
	<!-- ====================================================================== -->
	<xs:complexType name="AppealEventType">
