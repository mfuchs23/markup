			<xs:element name="ApplicationDate" type="xs:date" minOccurs="0"/>
			<xs:element name="RegistrationDate" type="xs:date" minOccurs="0"/>
			<xs:element name="PublicationDate" type="xs:date" minOccurs="0"/>
			<xs:element name="DesignURI" type="URIType" minOccurs="0"/>
			<xs:element name="DesignDescription" type="xs:string" minOccurs="0"/>
			<xs:element name="VerbalElementText" type="TextType" minOccurs="0"/>
			<xs:element name="PublicationDefermentIndicator" type="xs:boolean" minOccurs="0"/>
