  <!-- Allgemeine Attribute fuer Fachobjekte -->
  <xsd:attributeGroup name="BasisAttribute">
    <xsd:attribute name="INID" type="INIDType" use="optional" />
  </xsd:attributeGroup>

  <xsd:attribute name="suchfeld" type="xsd:NMTOKEN" />

